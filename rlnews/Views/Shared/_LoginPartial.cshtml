@if (Session["UserId"] != null)
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink(Session["Username"].ToString(), "Index", "Account", routeValues: null, htmlAttributes: new { id = "registerLink", @class = "nav-btn" })</li>
        <li>@Html.ActionLink("Logout", "Logout", "Account", routeValues: null, htmlAttributes: new { id = "loginLink", @class = "nav-btn nav-btn-last" })</li>
    </ul>
}
else {
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "Account", routeValues: null, htmlAttributes: new { id = "registerLink", @class = "nav-btn" })</li>
        <li>@Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { id = "loginLink", @class = "nav-btn nav-btn-last" })</li>
    </ul>
}
